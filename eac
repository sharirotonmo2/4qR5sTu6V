class MultidimensionalArray {
    public static void main(String[] args) {

        //
        int[][] a = {
            {1, -2, 3}, 
            {-4, -5, 6, 9}, 
            {7}, 
        };
      
        //for ... each
        //
        for (int[] innerArray: a) {
            //for ... eac
            for(int data: innerArray) {
                System.out.println(data);
            }
        }
    }
}
